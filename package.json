{
  "name": "@mars-man/components",
  "version": "1.0.1",
  "description": "mars-man components including inputs and route guards",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "test": "jest --config package.json --no-cache",
    "prepublish": "tsc -b",
    "build": "tsc -b"
  },
  "author": "Marc Frame",
  "license": "MIT",
  "dependencies": {
    "@mars-man/models": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "mobx": "^6.3.5",
    "mobx-react": "^7.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "mobx": "^5.9.4",
    "mobx-react": "^6.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "verbose": false,
    "testPathIgnorePatterns": [
      "testing"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@mars-man/.*))"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mars-manjs/components.git"
  },
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/mars-manjs/components/issues"
  },
  "homepage": "https://github.com/mars-manjs/components#readme"
}
